{
    "name": "mindforge-guard-workspace",
    "lockfileVersion": 3,
    "requires": true,
    "packages": {
        "": {
            "name": "mindforge-guard-workspace",
            "workspaces": [
                "packages/*"
            ],
            "engines": {
                "node": ">=18"
            }
        },
        "node_modules/@mindforge/guard": {
            "resolved": "packages/guard",
            "link": true
        },
        "node_modules/@mindforge/kernel": {
            "resolved": "packages/kernel",
            "link": true
        },
        "packages/guard": {
            "name": "@mindforge/guard",
            "version": "1.0.0",
            "bin": {
                "guard": "src/runGuard.mjs"
            }
        },
        "packages/kernel": {
            "name": "@mindforge/kernel",
            "version": "0.20.0"
        }
    }
}
